<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<project>
    <logs>
	<console>
	    <level>info</level> 
	    <pattern>[%thread] %-5level %msg %n</pattern>
	</console>
	<file>
	    <name>montecarlo.log</name> 
	    <level>info</level> 
	    <pattern>[%thread] %-5level %msg %n</pattern>
	    <overwrite>true</overwrite>
	</file>
    </logs>
   
    <models>
	<model id="Broadwick Project">
	    <classname>broadwick.example.montecarlo.App</classname>
	    <parameter id="numSimulations" value="1000000" />
	</model>
    </models>
</project>


